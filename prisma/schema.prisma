// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.sqlite"
}

model Article {
  id      Int    @id @default(autoincrement())
  course  String
  name    String
  birth   String
  email   String
  adress  String
  postNr  String
  message String
  phoneNr String
}
